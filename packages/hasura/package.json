{
  "name": "hasura",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "docker-compose up",
    "down": "docker-compose down",
    "down:with-volumes": "docker-compose down --volumes",
    "console": "hasura console --skip-update-check",
    "metadata": "hasura metadata apply --skip-update-check",
    "metadataReload": "hasura metadata reload --skip-update-check",
    "migrate": "hasura migrate apply --database-name default --skip-update-check",
    "status": "hasura migrate status --database-name default",
    "seed:apply:kansei": "hasura seed apply --file sual-kansei-questions.sql --database-name default --skip-update-check",
    "seed:apply:TEST_1": "hasura seed apply --file TEST_1.sql --database-name default",
    "seed:apply:TEST_2": "hasura seed apply --file TEST_2.sql --database-name default",
    "stg:metadata": "hasura metadata apply --envfile .hasura.env.stg --skip-update-check",
    "stg:status": "hasura migrate status --database-name default --envfile .hasura.env.stg --skip-update-check",
    "stg:delete:migrate": "hasura migrate delete --all --server --database-name default --envfile .hasura.env.stg",
    "stg:migrate": "hasura migrate apply --database-name default --envfile .hasura.env.stg --skip-update-check",
    "stg:seed:apply:kansei": "hasura seed apply --file sual-kansei-questions.sql --database-name default --envfile .hasura.env.stg --skip-update-check"
  },
  "keywords": [],
